{
  "name": "@worktools/ruled-router",
  "version": "0.2.28",
  "description": "Router parser designed and customized for web apps in JiMeng.io",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "webpack serve --hot --config webpack/dev.js",
    "watch": "webpack serve --hot --config webpack/dev.js",
    "release": "rm -rfv dist/* && time webpack --config webpack/release.js",
    "build": "yarn release",
    "upload": "rsync -avr --progress dist/ tiye.me:~/repo/worktools/ruled-router",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react",
    "test": "yarn jest",
    "format": "prettier src/*.ts --write",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worktools/ruled-router.git"
  },
  "keywords": [
    "router"
  ],
  "author": "jiyinyiyong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worktools/ruled-router/issues"
  },
  "homepage": "https://github.com/worktools/ruled-router#readme",
  "peerDependencies": {
    "@types/react": "*",
    "react": "*"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/router-code-generator": "^0.3.0",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.3.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@worktools/doc-frame": "^0.1.14-a2",
    "@worktools/flex-styles": "^0.2.1",
    "@worktools/jimo-basics": "^0.1.12-a1",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.2.4",
    "dayjs": "^1.10.4",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^6.2.9",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.4",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "typescript-styled-plugin": "^0.17.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "query-string": "^7.0.0"
  }
}
